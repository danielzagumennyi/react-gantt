{
  "name": "r7-gantt",
  "version": "0.3.1",
  "homepage": "https://ahwgs.github.io/react-gantt/#/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahwgs/react-gantt.git"
  },
  "license": "MIT",
  "author": "ahwgs <ah_wgs@126.com>",
  "module": "dist/index.esm.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "check:ts": "tsc --noEmit",
    "build": "cross-env NODE_ENV=production avenger build",
    "build:website": "dumi build",
    "release": "bumpp package.json --commit --push --tag",
    "start": "cross-env NODE_ENV=development avenger build --watch",
    "start:website": "dumi dev"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "*.{js,json,md,tsx,ts}": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.24.7",
    "ahooks": "^3.8.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.11",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mobx": "6.12.3",
    "mobx-react-lite": "4.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@osdoc-dev/avenger-cli": "^0.2.0",
    "@osdoc-dev/eslint-config-preset-prettier": "1.0.5-beta.0",
    "@osdoc-dev/eslint-config-preset-react": "1.0.5-beta.0",
    "@types/classnames": "^2.2.11",
    "@types/lodash": "^4.14.165",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-loader": "^8.2.2",
    "bumpp": "^7.1.1",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "cz-customizable": "^6.3.0",
    "dumi": "^1.1.23",
    "husky": "^4.3.6",
    "identity-obj-proxy": "^3.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.1.14",
    "postcss-url": "^10.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "rollup-plugin-postcss": "^4.0.0",
    "tslib": "^2.6.3",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "engines": {
    "node": ">=v18.7.0"
  }
